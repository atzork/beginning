{

  "local-config-enabled": true,

  "server": {
    "host": "127.0.0.1",
    "port": "1337"
  },

  "db": {

    "redis": {
      "port"    : 6379,
      "host"    : "127.0.0.1",
      "option" : {
        "db"    : 1,
        "parser": "hiredis"
      },
      "cacheKey" : "rc",
      "liveTime": 3600
    },

    "mongodb": {
      "url"   : "mongodb://localhost/beginning",
      "option": {
        "host"          : "localhost",
        "port"          : 27017,
        "autoReconect"  : true,
        "socketOptions" : {
          "keepAlive" : 1
        }
      }
    }

  }
}
